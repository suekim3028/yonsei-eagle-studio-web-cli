{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb", "prettier", "plugin:@next/next/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "requireConfigFile": false
  },
  "parser": "@babel/eslint-parser",
  "plugins": ["react", "import"],
  "rules": {
    "@typescript-eslint/consistent-type-definitions": "off"
  }
}
